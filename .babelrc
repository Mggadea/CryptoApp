{
  "plugins": [
    [
      "module-resolver",
      {
        "cwd": "babelrc",
        "root": ["./src"],
        "extensions": [".js",".ts", "tsx", ".ios.js", ".android.js"],
        "alias": {
          "@navigation": "./src/navigation",
          "@screens": "./src/screens",
          "@styles": "./src/assets/styles",
          "@assets": "./src/assets",
          "@services": "./src/services",
          "@components": "./src/components",
          "@context": "./src/context",
        }
      }
    ]
  ]
}
